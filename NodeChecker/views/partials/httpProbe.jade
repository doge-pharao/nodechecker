div(class="panel panel-success")
		.panel-heading
			.row
				.col-xs-8
					a(href="javascript:;" data-toggle="collapse" data-parent="#accordion" data-target="#{{probe._id}}")
						span(style="color:black") {{probe.name}}
				.col-xs-4
					.row
						.col-xs-6
							p {{probe.enabled}}
						.col-xs-3
							label(for="{{probe._id}}_radio_enabled")
								input(type="radio", name="{{probe._id}}_radio_enabled", value="true")
								|	ON
						.col-xs-3
							label(for="{{probe.id}}_radio_enabled")
								input(type="radio", name="{{probe._id}}_radio_enabled", value="false")
								|	OFF
		.panel-body.panel-collapse.collapse.out(id="{{probe._id}}")
			div(ng-switch="probe.method.type")
				div(ng-switch-when="GET")
					div(ng-include="'partials/http/getMethod'")
				div(ng-switch-when="POST")
					div(ng-include="'partials/http/postMethod'")
				div(ng-switch-default)
					p {{probe.method.type}}
